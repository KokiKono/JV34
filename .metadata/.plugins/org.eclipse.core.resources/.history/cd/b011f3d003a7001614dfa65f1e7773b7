package beans;

public interface LoginInterface {
	/**
	 * ログインチャレンジを行うSQLファイルを定義する。
	 * @author 浩生
	 *
	 */
	public enum Dao{
		SQL("SelectLogin.sql");

		public String sqlPath;
		Dao(String sqlPath){
			this.sqlPath=sqlPath;
		}
	}
	/**
	 * ログイン状態によって初期フォワード先を定義する。
	 * @author 浩生
	 *
	 */
	public enum Forward{

		Welcome("0","welcome.jsp");
		public String flg;
		public String to;
		Forward(String flg,String to){
			this.flg=flg;
			this.to=to;
		}
	}

}
