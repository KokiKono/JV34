package beans;

public interface LoginInterface {
	public enum Dao{
		SQL("SelectLogin.sql");

		public String sqlPath;
		Dao(String sqlPath){
			this.sqlPath=sqlPath;
		}
	}
	public enum Forward{


		public String flg;
		public String from;
		public String to;
		Forward(String flg,String from,String to){
			this.flg=flg;
			this.from=from;
			this.to=to;
		}
	}
}
